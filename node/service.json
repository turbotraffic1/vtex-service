{
  "memory": 512,
  "ttl": 900000,
  "timeout": 900000,
  "minReplicas": 10,
  "maxReplicas": 50,
  "workers": 4,
  "routes": {
    "versionFetcher": {
      "path": "/t/version",
      "public": true
    },
    "keywordFetcher": {
      "path": "/t/:keyword",
      "public": true
    },
    "xmlFetcher": {
      "path": "/t/xmls/:index",
      "public": true
    },
    "sitemapFetcher": {
      "path": "/t/sitemap",
      "public": true
    }
  }
}